

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Budget {
  id     Int     @id @default(autoincrement())
  amount Float
}

model Expense {
  id          Int      @id @default(autoincrement())
  description String
  amount      Float
  createdAt   DateTime @default(now())
}


