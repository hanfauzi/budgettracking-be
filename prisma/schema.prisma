

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String   
  createdAt DateTime @default(now())
}


model Budget {
  id     Int     @id @default(autoincrement())
  amount Float
}

model Expense {
  id          Int      @id @default(autoincrement())
  description String
  amount      Float
  createdAt   DateTime @default(now())
}


